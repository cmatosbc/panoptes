{
    "name": "panoptes/async-curl",
    "description": "Asynchronous HTTP Client using PHP Fibers and cURL",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Panoptes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Panoptes\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "license": "gpl-3.0-or-later",
    "authors": [
        {
            "name": "Carlos Matos",
            "email": "carlosarturmatos1977@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
